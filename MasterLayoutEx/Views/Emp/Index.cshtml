@model IEnumerable<MasterLayoutEx.Models.EMPDATA>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<p>@Html.ActionLink("Create New","Create","Emp")</p>
@using(Html.BeginForm())
{
    <table>
        <tr>
            <td>Empno</td>
            <td>Ename</td>
            <td>Job</td>
            <td>Mgr</td>
            <td>Hiredate</td>
            <td>Salary</td>
            <td>Commission</td>
            <td>Deptno</td>
        </tr>

        @foreach(var E in Model)
        {
    <tr>
        <td>@E.EMPNO</td>
        <td>@E.ENAME</td>
        <td>@E.JOB</td>
        <td>@E.MGR</td>
        <td>@E.HIREDATE</td>
        <td>@E.SAL</td>
        <td>@E.COMM</td>
        <td>@E.DEPTNO</td>
        <td>
            @Html.ActionLink("Edit Emp","Edit","Emp",new { id = E.EMPNO },null)
        </td>
        <td>
            @Html.ActionLink("Delete Emp", "Delete", "Emp", new { id = E.EMPNO }, null)
        </td>
    </tr>
        }
    </table>

    if(ViewBag.msg!=null)
    {
        <h3>@ViewBag.msg</h3>
    }
}
