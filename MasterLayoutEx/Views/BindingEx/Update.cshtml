@model MasterLayoutEx.Models.EMPDATA
@{
    ViewBag.Title = "Update";
}

<h2>Update</h2>
@using (Html.BeginForm())
{
    <table>
        <tr><td>Empno</td><td>@Html.TextBoxFor(a => a.EMPNO)</td></tr>
        <tr><td>Ename</td><td>@Html.TextBoxFor(a => a.ENAME)</td></tr>
        <tr><td>Job</td><td>@Html.TextBoxFor(a => a.JOB)</td></tr>
        <tr><td>Mgr</td><td>@Html.TextBoxFor(a => a.MGR)</td></tr>
        <tr><td>Hiredate</td><td>@Html.TextBoxFor(a => a.HIREDATE)</td></tr>
        <tr><td>sal</td><td>@Html.TextBoxFor(a => a.SAL)</td></tr>
        <tr><td>Comm</td><td>@Html.TextBoxFor(a => a.COMM)</td></tr>
        <tr><td>Deptno</td><td>@Html.TextBoxFor(a => a.DEPTNO)</td></tr>
        <tr><td><input type="submit" value=" add" /></td></tr>
    </table>

    if (ViewBag.msg != null)
    {
        <h3>@ViewBag.msg</h3>
    }
}



